	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug Config parser",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/Cosmos/customBox/newGenerator/Parser/ConfigParser.py",
			"args": [
				"${workspaceFolder}/Cosmos/configuration/workspace.json"
			],
			"console": "integratedTerminal"
		},
		{
			"name": "Debug Default config generator",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/Cosmos/customBox/newGenerator/DefaultConfig.py",
			"args": [
				"-c",
				"${workspaceFolder}/Cosmos/configuration/workspace.json"
			],
			"console": "integratedTerminal"
		},
		{
			"name": "Debug Generator",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/Cosmos/customBox/newGenerator/generator.py",
			"args": [
				"${workspaceFolder}/Cosmos/configuration/workspace.json"
			],
			"console": "integratedTerminal"
		}
	]
	}
